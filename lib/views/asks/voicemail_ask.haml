%h3= "#{index} - #{ask.title} - Request a voice message has " "#{ask.recipients.count} recipients and #{ask.respondents.count} respondents"
#ask-information
  =haml :message, locals: {message: ask.message}
  %h4 Prompt : "#{ask.prompt}"
  %h4 Voicemails:
  -ask.voicemails.each do |vm|
    %a{href: vm.filename}= vm.id
